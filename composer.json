{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type":"vcs",
            "url":"https://github.com/mcaskill/composer-merge-plugin"
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/font",
                "type": "drupal-library",
                "version": "4.13.1",
                "dist": {
                    "url": "https://github.com/drupal-ckeditor-libraries-group/font/archive/major.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/colorbutton",
                "type": "drupal-library",
                "version": "4.15",
                "dist": {
                    "url": "https://download.ckeditor.com/colorbutton/releases/colorbutton_4.15.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/panelbutton",
                "type": "drupal-library",
                "version": "4.15",
                "dist": {
                    "url": "https://download.ckeditor.com/panelbutton/releases/panelbutton_4.15.0.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "ckeditor/colorbutton": "^4.15",
        "ckeditor/font": "^4.13",
        "ckeditor/panelbutton": "^4.15",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal-developer/web_corporate": "^1.0",
        "drupal/admin_toolbar": "^3.2",
        "drupal/advagg": "4.x-dev",
        "drupal/antibot": "^2.0",
        "drupal/blazy": "^2.21",
        "drupal/blazy_ui": "^2.21",
        "drupal/bootstrap5": "^4.0@alpha",
        "drupal/ckeditor_font": "^2.0@beta",
        "drupal/colorbox": "^2.0",
        "drupal/colorbox_load": "^1.3",
        "drupal/colorbutton": "^1.2",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_split": "^2.0@RC",
        "drupal/cookies": "^1.0",
        "drupal/core-composer-scaffold": "^9.4",
        "drupal/core-recommended": "^9.4",
        "drupal/ctools": "^4.0",
        "drupal/devel": "^5.0",
        "drupal/devel_php": "^1.3",
        "drupal/entity_reference_revisions": "^1.10",
        "drupal/field_group": "^3.1",
        "drupal/filelog": "2.0.x-dev",
        "drupal/filename_transliteration": "^1.1",
        "drupal/filter_perms": "^2.0@RC",
        "drupal/fontawesome": "^2.26",
        "drupal/fontawesome_iconpicker_widget": "^2.26",
        "drupal/fontawesome_media": "^2.26",
        "drupal/form_alter_service": "^3.0",
        "drupal/geolocation": "^3.2",
        "drupal/gin": "^3.0@beta",
        "drupal/gin_login": "^2.0",
        "drupal/gin_toolbar": "^1.0@beta",
        "drupal/google_tag": "^1.4",
        "drupal/imce": "^3.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jquery_ui": "^1.4",
        "drupal/jquery_ui_accordion": "^2.0",
        "drupal/jquery_ui_checkboxradio": "^1.2",
        "drupal/jquery_ui_datepicker": "^1.0",
        "drupal/jquery_ui_tabs": "^1.1",
        "drupal/jquery_ui_tooltip": "^1.1",
        "drupal/mail_login": "^2.2",
        "drupal/mailsystem": "^4.3",
        "drupal/metatag": "^1.15",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/panelbutton": "^1.3",
        "drupal/paragraphs": "^1.16",
        "drupal/pathauto": "^1.8",
        "drupal/phpmailer_smtp": "^2.0",
        "drupal/private_files_download_permission": "^3.0",
        "drupal/quick_node_clone": "^1.16",
        "drupal/redirect": "^1.6",
        "drupal/redirect_after_login": "^2.7",
        "drupal/responsive_favicons": "1.x-dev",
        "drupal/schema_metatag": "^2.2",
        "drupal/simple_pass_reset": "^1.1",
        "drupal/simple_sitemap": "^4.1",
        "drupal/slick": "^2.10",
        "drupal/slick_paragraphs": "^2.4",
        "drupal/structure_sync": "^2.0",
        "drupal/svg_image": "^3.0",
        "drupal/svg_image_responsive": "^3.0",
        "drupal/token": "^1.9",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.0",
        "drupal/twig_vardumper": "^3.0",
        "drupal/user_registrationpassword": "^2.0@alpha",
        "drupal/video": "^3.0",
        "drupal/views_bootstrap": "^5.5@alpha",
        "drupal/vppr": "1.x-dev",
        "drupal/webform": "^6.2@beta",
        "drupal/webp": "^1.0@beta",
        "drupal/whatsapp_bubble": "^1.0@beta",
        "drush/drush": "^10.3",
        "vlucas/phpdotenv": "^5.2",
        "wikimedia/composer-merge-plugin": "dev-feature/composer-v2 as 1.5.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "autoload": {
        "files": ["load.environment.php"]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/robots.txt": {
                    "mode": "skip"
                },
                "[web-root]/sites/default/default.services.yml": {
                    "mode": "skip"
                },
                "[web-root]/sites/default/default.settings.php": {
                    "mode": "skip"
                }
            }
        },
        "patches": {
            "drupal/redirect_after_login": {
                "Headers have already been sent after upgrade to Drupal 9.2 (can't login)": "patches/redirect_after_login_cant_login.patch"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/modules/core/{$name}": [
                "type:drupal-module-core"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ]
        },
        "merge-plugin": {
            "include": [
                "web/modules/contrib/webform/composer.libraries.json"
            ],
            "recurse": true,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-scripts": false
        }
    }
}
